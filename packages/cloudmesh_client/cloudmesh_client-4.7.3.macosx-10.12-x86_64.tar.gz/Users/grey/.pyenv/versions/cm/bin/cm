#!/Users/grey/.pyenv/versions/cm/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'cloudmesh-client==4.7.3','console_scripts','cm'
__requires__ = 'cloudmesh-client==4.7.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('cloudmesh-client==4.7.3', 'console_scripts', 'cm')()
    )
