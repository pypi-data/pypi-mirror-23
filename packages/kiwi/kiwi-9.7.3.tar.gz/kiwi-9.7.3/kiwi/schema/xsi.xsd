<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <xs:import schemaLocation="kiwi.xsd"/>
  <xs:attribute name="noNamespaceSchemaLocation" type="xs:anyURI">
    <xs:annotation>
      <xs:documentation>The location of the XSD Schema (not relevant for RELAX NG or DTD)</xs:documentation>
    </xs:annotation>
  </xs:attribute>
  <xs:attribute name="schemaLocation" type="xs:anyURI">
    <xs:annotation>
      <xs:documentation>A pair of URI references: First is a namespace name,
second the location of the XSD Schema
(not relevant for RELAX NG or DTD)</xs:documentation>
    </xs:annotation>
  </xs:attribute>
</xs:schema>
