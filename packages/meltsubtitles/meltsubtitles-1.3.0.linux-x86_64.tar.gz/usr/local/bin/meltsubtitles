#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'meltsubtitles==1.3.0','console_scripts','meltsubtitles'
__requires__ = 'meltsubtitles==1.3.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('meltsubtitles==1.3.0', 'console_scripts', 'meltsubtitles')()
    )
