#!/home/ralph/bin/python/python35/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'meltsubtitles==1.4.0','console_scripts','meltsubtitles'
__requires__ = 'meltsubtitles==1.4.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('meltsubtitles==1.4.0', 'console_scripts', 'meltsubtitles')()
    )
