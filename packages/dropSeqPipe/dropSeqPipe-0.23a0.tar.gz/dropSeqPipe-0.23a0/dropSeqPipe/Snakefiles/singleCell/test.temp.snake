"""Generate fastqc reports."""


# Configfile
configfile:'config.yaml'

# Executables
FASTQC = config['FASTQCEXEC']



rule all:
    input: expand('fastqc/{sample}.test', sample=config['Samples'])

rule fastqc:
    """Create fastqc report"""
    input:
        r1='{sample}_R1.fastq.gz',
        r2='{sample}_R2.fastq.gz'
    output: 'fastqc/{sample}.test'
    threads: 1
    script:
        """../../Rscripts/test.R"""