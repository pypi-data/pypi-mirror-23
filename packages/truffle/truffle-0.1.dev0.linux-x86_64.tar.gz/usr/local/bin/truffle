#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'truffle==0.1.dev0','console_scripts','truffle'
__requires__ = 'truffle==0.1.dev0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('truffle==0.1.dev0', 'console_scripts', 'truffle')()
    )
