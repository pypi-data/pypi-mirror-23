{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        //"include": "../js",
        "includePattern": ".+\\.js(doc)?$",
        //"includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "/usr/local/lib/node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}