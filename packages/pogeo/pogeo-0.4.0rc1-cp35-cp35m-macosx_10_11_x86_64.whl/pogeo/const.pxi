DEF CELL_RADIUS = 500

DEF PI = 3.14159265358979323846264338327950288
DEF EARTH_RADIUS_KILOMETERS = 6371.0088
DEF EARTH_RADIUS_METERS = 6371008.8
DEF EARTH_RADIUS_MILES = EARTH_RADIUS_KILOMETERS * 0.621371
DEF AXIS_HEIGHT = ((CELL_RADIUS / EARTH_RADIUS_METERS) ** 2) / 2.0
DEF RAD_TO_DEG = 180.0 / PI
DEF DEG_TO_RAD = PI / 180.0
