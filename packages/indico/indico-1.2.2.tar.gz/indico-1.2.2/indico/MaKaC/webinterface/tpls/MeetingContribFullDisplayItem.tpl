<table width="100%" cellpadding="1" cellspacing="0" border="0">
    <tr>
        <td bgcolor="#000060">
            <table width="100%" cellpadding="4" cellspacing="0" border="0" bgcolor="white">
                <tr>
                    <td align="center" valign="top" width="1%" nowrap><font color="black"><b>&nbsp;${ startDate }</b></font></td>
                    <td colspan="2" width="100%" valign="top" bgcolor="#F6F6F6">
                        <table width="100%" cellpadding="2" cellspacing="0" border="0"
                            bgcolor="#F6F6F6">
                            <tr>
                                <td valign="top" width="100%"><font class="headline">${ modifyItem }<b>${ title }</b></font><small> <font color="red">(${ duration })</font> ${ materials }</small><small>${ place }</small></td>
                                <td align="right" nowrap>${ speakerList }</td>
                            </tr>
                            <tr>
                                <td valign="top" colspan="2">
                                    ${ description }
                                </td>
                            </tr>
                            <tr>
                                <td width="70%" colspan="2">${ subConts }</td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>
