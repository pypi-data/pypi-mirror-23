{
  "document_id": "doc_id",
  "extraction_policy": "replace",
  "error_handling": "raise_error",
  "resources": {
    "dictionaries": {
      "city": "city.json.gz",
      "state_to_country": "state_country_dict.json",
      "geonames": "city_dict_alt_15000.json",
      "countries": "countries.json.gz",
      "ethnicities": "ethnicities.json.gz",
      "haircolors": "haircolors.json.gz",
      "adult_services": "adult_services.json.gz",
      "eyecolors": "eyecolors.json.gz",
      "states_usa_canada": "states_usa_canada.json.gz",
      "states_usa_codes": "states_usa_codes.json.gz",
      "state_to_codes_lower": "states-to-codes-lower.json",
      "country_code": "country_codes_dict.json"
    },
    "landmark": [
      "consolidated_rules.json"
    ],
    "spacy_field_rules": {
      "name": "spacy_field_rules.json"
    }
  },
  "data_extraction": [
    {
      "fields": {
        "name": {
          "extractors": {
            "extract_using_custom_spacy": {
              "config": {
                "spacy_field_rules": "name"
              }
            }
          }
        }
      },
      "input_path": [
        "*.content_strict.text.`parent`",
        "*.content_relaxed.text.`parent`",
        "*.title.text.`parent`"
      ]
    },
    {
      "input_path": [
        "*.content_strict.text.`parent`",
        "*.content_relaxed.text.`parent`",
        "*.title.text.`parent`",
        "*.inferlink_extractions.*.text.`parent`"
      ],
      "fields": {
        "social_media_id": {
          "extractors": {
            "extract_using_spacy": {
              "config": {}
            }
          }
        },
        "country": {
          "extractors": {
            "extract_using_dictionary": {
              "config": {
                "dictionary": "countries"
              }
            }
          }
        },
        "review_id": {
          "extractors": {
            "extract_review_id": {
              "config": {}
            },
            "extract_from_landmark": {
              "config": {
                "fields": [
                  "inferlink_review-id"
                ]
              }
            }
          }
        },
        "service": {
          "extractors": {
            "extract_using_dictionary": {
              "config": {
                "dictionary": "adult_services",
                "ngrams": 3
              }
            },
            "extract_from_landmark": {
              "config": {
                "fields": [
                  "inferlink_service"
                ]
              }
            }
          }
        },
        "state": {
          "extractors": {
            "extract_using_dictionary": {
              "config": {
                "dictionary": "states_usa_canada",
                "ngrams": 3
              }
            },
            "extract_from_landmark": {
              "config": {
                "fields": [
                  "inferlink_state"
                ]
              }
            }
          }
        },
        "eye_color": {
          "extractors": {
            "extract_using_dictionary": {
              "config": {
                "dictionary": "eyecolors",
                "ngrams": 2
              }
            },
            "extract_from_landmark": {
              "config": {
                "fields": [
                  "inferlink_eye-color"
                ]
              }
            }
          }
        },
        "hair_color": {
          "extractors": {
            "extract_using_dictionary": {
              "config": {
                "dictionary": "haircolors",
                "ngrams": 1
              }
            },
            "extract_from_landmark": {
              "config": {
                "fields": [
                  "inferlink_hair-color"
                ]
              }
            }
          }
        },
        "ethnicity": {
          "extractors": {
            "extract_using_dictionary": {
              "config": {
                "dictionary": "ethnicities",
                "ngrams": 3
              }
            },
            "extract_from_landmark": {
              "config": {
                "fields": [
                  "inferlink_ethnicity"
                ]
              }
            }
          }
        },
        "city_name": {
          "extractors": {
            "extract_using_dictionary": {
              "config": {
                "dictionary": "city",
                "ngrams": 3
              }
            },
            "extract_from_landmark": {
              "config": {
                "fields": [
                  "inferlink_city"
                ]
              }
            }
          }
        },
        "states_usa_codes": {
          "extractors": {
            "extract_using_dictionary": {
              "config": {
                "ngrams": 1,
                "dictionary": "states_usa_codes"
              }
            }
          }
        },
        "posting_date": {
          "extractors": {
            "extract_using_spacy": {
              "config": {
                "post_filter": [
                  "parse_date"
                ]
              }
            },
            "extract_from_landmark": {
              "config": {
                "fields": [
                  "inferlink_posting-date",
                  "inferlink_posting-date-2",
                  "inferlink_posting-date-1"
                ],
                "post_filter": [
                  "parse_date"
                ]
              }
            }
          }
        },
        "name": {
          "extractors": {
            "extract_from_landmark": {
              "config": {
                "fields": [
                  "inferlink_name",
                  "inferlink_name-1"
                ],
                "post_filter": [
                  "x.replace('\t', '')"
                ]
              },
              "extraction_policy": "replace"
            },
            "extract_using_custom_spacy": {
              "config": {
                "spacy_field_rules": "name"
              }
            }
          }
        },
        "age": {
          "extractors": {
            "extract_from_landmark": {
              "config": {
                "fields": [
                  "inferlink_age",
                  "inferlink_age-1",
                  "inferlink_age-2"
                ]
              },
              "extraction_policy": "replace"
            },
            "extract_age": {
              "config": {}
            },
            "extract_using_spacy": {
              "config": {}
            }
          }
        },
        "phone": {
          "extractors": {
            "extract_phone": {
              "config": {}
            },
            "extract_from_landmark": {
              "config": {
                "fields": [
                  "inferlink_phone",
                  "inferlink_local-phone",
                  "inferlink_phone-1"
                ],
                "post_filter": [
                  "extract_phone"
                ]
              },
              "extraction_policy": "replace"
            }
          }
        },
        "email": {
          "extractors": {
            "extract_email": {
              "config": {}
            }
          }
        },
        "price": {
          "extractors": {
            "extract_price": {
              "config": {},
              "extraction_policy": "replace"
            }
          }
        },
        "height": {
          "extractors": {
            "extract_height": {
              "config": {},
              "extraction_policy": "replace"
            },
            "extract_from_landmark": {
              "config": {
                "fields": [
                  "inferlink_height"
                ],
                "post_filter": [
                  "extract_height"
                ]
              }
            }
          }
        },
        "weight": {
          "extractors": {
            "extract_weight": {
              "config": {},
              "extraction_policy": "replace"
            },
            "extract_from_landmark": {
              "config": {
                "fields": [
                  "inferlink_weight"
                ],
                "post_filter": [
                  "extract_weight"
                ]
              }
            }
          }
        },
        "address": {
          "extractors": {
            "extract_using_spacy": {
              "config": {}
            }
          }
        },
        "gender": {
          "extractors": {
            "extract_from_landmark": {
              "config": {
                "fields": [
                  "inferlink_gender"
                ]
              }
            }
          }
        }
      }
    },
    {
      "input_path": [
        "*.url.text.`parent`"
      ],
      "fields": {
        "website": {
          "extractors": {
            "extract_website_domain": {
              "config": {}
            }
          }
        },
        "state": {
          "extractors": {
            "extract_using_dictionary": {
              "config": {
                "dictionary": "state"
              }
            }
          }
        },
        "city_name": {
          "extractors": {
            "extract_using_dictionary": {
              "config": {
                "dictionary": "city"
              }
            }
          }
        },
        "country": {
          "extractors": {
            "extract_country_url": {
              "config": {
                "dictionary": "stop_words"
              }
            },
            "extract_using_dictionary": {
              "config": {
                "dictionary": "country"
              }
            }
          }
        }
      }
    }
  ],
  "kg_enhancement": {
    "fields": {
      "city": {
        "priority": 1,
        "extractors": {
          "create_city_state_country_triple": {
            "config": {}
          }
        }
      },
      "populated_places": {
        "priority": 0,
        "extractors": {
          "geonames_lookup": {
            "config": {}
          }
        }
      }
    },
    "input_path": "knowledge_graph.`parent`"
  }
}